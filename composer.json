{
  "name": "spryker-shop/shared-cart-page",
  "type": "library",
  "description": "SharedCartPage module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker-shop/cart-page": "^1.0.0",
    "spryker-shop/shop-application": "^1.0.0",
    "spryker-shop/shop-ui": "^1.0.0",
    "spryker/application": "^3.0.0",
    "spryker/company-user": "^2.1.1",
    "spryker/customer": "^7.0.0",
    "spryker/kernel": "^3.19.0",
    "spryker/multi-cart": "^1.1.0",
    "spryker/shared-cart": "^1.4.0",
    "spryker/symfony": "^3.1.0"
  },
  "require-dev": {
    "spryker-shop/multi-cart-page": "*",
    "spryker/code-sniffer": "*",
    "spryker/silex": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker-shop/multi-cart-page": "if you want to use customer layout templates: ^1.1.0",
    "spryker/permission": "If you want to use permissions plugins",
    "spryker/silex": "Use this module when using plugins that need Silex dependencies."
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
